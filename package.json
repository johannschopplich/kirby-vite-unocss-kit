{
  "prrivate": true,
  "scripts": {
    "assets:unocss": "esmo scripts/generateUnoCSS.ts",
    "dev": "shx touch src/.lock && concurrently \"npm run assets:unocss\" \"vite\"",
    "build": "shx rm -f src/.lock && NODE_ENV=production npm run assets:unocss && vite build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@unocss/preset-icons": "^0.7.2",
    "colorette": "^2.0.16",
    "concurrently": "^6.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "esmo": "^0.12.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "shx": "^0.3.3",
    "unocss": "^0.7.2",
    "vite": "^2.6.13",
    "vite-plugin-full-reload": "^0.2.2"
  }
}
