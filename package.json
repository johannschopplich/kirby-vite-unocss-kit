{
  "private": true,
  "scripts": {
    "uno": "unocss \"site/{snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "rm -rf public/dist && concurrently \"npm run uno -- --watch\" \"vite\"",
    "build": "npm run uno && vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.6"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^18.15.5",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.6",
    "sass": "^1.59.3",
    "shx": "^0.3.4",
    "unocss": "^0.50.6",
    "vite": "^4.2.1",
    "vite-plugin-full-reload": "^1.0.5"
  }
}
