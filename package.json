{
  "prrivate": true,
  "scripts": {
    "uno": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run uno -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run uno && vite build",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.41.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.6",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@unocss/preset-icons": "^0.41.1",
    "@unocss/preset-typography": "^0.41.1",
    "@unocss/preset-wind": "^0.41.1",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "shx": "^0.3.4",
    "unocss": "^0.41.1",
    "vite": "^3.0.0-beta.5",
    "vite-plugin-full-reload": "^1.0.0"
  }
}
