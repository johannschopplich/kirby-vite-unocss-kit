{
  "prrivate": true,
  "scripts": {
    "uno": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run uno -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run uno && vite build",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.21"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^18.7.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "shx": "^0.3.4",
    "unocss": "^0.45.21",
    "vite": "^3.1.0",
    "vite-plugin-full-reload": "^1.0.4"
  }
}
