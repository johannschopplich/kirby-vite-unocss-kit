{
  "prrivate": true,
  "scripts": {
    "uno": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run uno -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run uno && vite build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@unocss/reset": "^0.12.9"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.433",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@unocss/preset-icons": "^0.12.9",
    "colorette": "^2.0.16",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "prettier": "^2.5.0",
    "sass": "^1.43.5",
    "shx": "^0.3.3",
    "unocss": "^0.12.9",
    "unocss-preset-typography": "^0.3.0",
    "vite": "^2.6.14",
    "vite-plugin-full-reload": "^0.2.2"
  }
}
