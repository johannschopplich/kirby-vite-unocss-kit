{
  "prrivate": true,
  "scripts": {
    "uno": "unocss \"site/{layouts,snippets,templates}/**/*\" --out-file src/styles/uno.css",
    "dev": "shx touch src/.lock && concurrently \"npm run uno -- --watch\" \"vite\"",
    "build": "shx rm -f src/.lock && npm run uno && vite build",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier \"src/**/*.{css,scss,ts}\" --write --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.32.8"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.3",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@unocss/preset-icons": "^0.32.8",
    "@unocss/preset-typography": "^0.32.8",
    "@unocss/preset-wind": "^0.32.8",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "shx": "^0.3.4",
    "unocss": "^0.32.8",
    "vite": "^2.9.6",
    "vite-plugin-full-reload": "^1.0.0"
  }
}
