{
  "require": {
    "php": ">=7.4.0",
    "getkirby/cms": "^3.6",
    "getkirby/layouts": "^1.0",
    "johannschopplich/kirby-extended": "^2.10",
    "johannschopplich/kirby-blurry-placeholder": "^1.4",
    "johannschopplich/kirby-vite": "^1.0"
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 -t public server.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "kirby-cms-path": false
  }
}
